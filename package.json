{
  "name": "phaser-game",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "game": "ionic serve",
    "vitest": "vitest",
    "clean": "rimraf electron/dist www/ dist/",
    "build:electron": "tsc -p electron/tsconfig.json",
    "start:electron": "electron electron/dist/main.js",
    "build:app": "ionic build",
    "electron:dev": "bun run build:app && bun run build:electron && bun run start:electron",
    "electron:build": "electron-builder",
    "build:app:prod": "ionic build --prod",
    "build:electron:prod": "tsc -p electron/tsconfig.prod.json",
    "prestart:prod": "bun run clean && bun run build:app:prod && bun run build:electron:prod",
    "start:prod": "NODE_ENV=production electron electron/dist-prod/main.js",
    "electron:prod": "bun run prestart:prod && bun run start:prod",
    "package:prod": "bun run prestart:prod && electron-builder --publish never",
    "package:win": "bun run prestart:prod && electron-builder --win --x64",
    "package:win-portable": "bun run prestart:prod && electron-builder --win --x64 --config.win.target=portable",
    "package:win-all": "bun run prestart:prod && electron-builder --win --x64 --ia32"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.5",
    "@angular/common": "19.2.5",
    "@angular/compiler": "19.2.5",
    "@angular/core": "19.2.5",
    "@angular/forms": "19.2.5",
    "@angular/platform-browser": "19.2.5",
    "@angular/platform-browser-dynamic": "19.2.5",
    "@angular/router": "19.2.5",
    "@capacitor/android": "7.2.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.2.0",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "7.2.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/angular": "8.5.3",
    "@ionic/cli": "^7.2.1",
    "ionicons": "7.4.0",
    "phaser": "3.88.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.6",
    "@angular-devkit/core": "19.2.6",
    "@angular-devkit/schematics": "19.2.6",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "19.2.6",
    "@angular/compiler-cli": "19.2.5",
    "@angular/language-service": "19.2.5",
    "@capacitor/cli": "7.2.0",
    "@ionic/angular-toolkit": "12.1.1",
    "@types/electron": "^1.6.12",
    "@types/jasmine": "5.1.7",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "@typescript-eslint/types": "^8.29.1",
    "@typescript-eslint/utils": "8.29.1",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "eslint": "9.24.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.9",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "5.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "description": "An Ionic project",
  "browserslist": [
    "Chrome >=79",
    "ChromeAndroid >=79",
    "Firefox >=70",
    "Edge >=79",
    "Safari >=14",
    "iOS >=14"
  ],
  "main": "electron/dist-prod/main.js",
  "build": {
    "appId": "com.msa.boomelo",
    "productName": "Boomelo",
    "files": [
      "electron/dist-prod/**/*",
      "www/browser/**/*",
      "assets/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon/icon.ico"
    }
  }
}
